CREATE TABLE `id21107995_music`.`contact` (`id` INT(255) NOT NULL AUTO_INCREMENT , `name` VARCHAR(255) NOT NULL , `email` VARCHAR(255) NOT NULL , `mobile` VARCHAR(255) NOT NULL , `subject` TEXT NOT NULL , `message` TEXT NOT NULL , PRIMARY KEY (`id`)) ;


CREATE TABLE `user` (`id` INT(255) NOT NULL AUTO_INCREMENT , `name` VARCHAR(255) NOT NULL , `email` VARCHAR(255) NOT NULL , `mobile` VARCHAR(255) NOT NULL , `password` VARCHAR(255) NOT NULL , `gender` TEXT NOT NULL , `education` VARCHAR(255) NOT NULL , `sub` VARCHAR(255) NOT NULL , PRIMARY KEY (`id`)) ;